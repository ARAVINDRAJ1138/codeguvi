#include<stdio.h>
void main()
 {
   int a[5],i,start=0,total=0;
   for(i=0;i<5;i++)
    {
      scanf("%d",a[i]);
    }
    
      start=a[0];
       while(start>4)
        {
          total=start+a[start];
          start=total;
        }
        if(start=4)
        printf("true");
      else
        printf("false");
  }
